      body {
        padding-top: 20px;
        padding-bottom: 40px;
      }

      .footer { text-align: center; font-size: 0.9em; }

      div.masthead { padding-top: 20px; }
      #brand { margin-top: -20px; }

      /* Custom container */
      .container-narrow {
        margin: 0 auto;
        max-width: 700px;
      }
      .container-narrow > hr {
        margin: 10px 0;
      }

      /* Main marketing message and sign up button */
      .jumbotron {
        margin: 40px 0;
        text-align: center;
      }
      .jumbotron h1 {
        font-size: 72px;
        line-height: 1;
      }
      .jumbotron .btn {
        font-size: 21px;
        padding: 10px 10px;
      }

      /* Supporting marketing content */
      .marketing {
        margin: 40px 0;
      }
      .marketing p + h4 {
        margin-top: 28px;
      }


      .marketing h4 { text-align: center; }

