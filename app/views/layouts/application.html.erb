    <script>
    function authenticateFacebook (next) {
      var win = window.open('/auth/facebook', '_blank','location=yes, scrollbars=yes, width=640, height=359', true);
      var timer = setInterval(function() {   
        if (win.closed) {  
          clearInterval(timer);
          next();
        }  
      }, 10);  
    }
