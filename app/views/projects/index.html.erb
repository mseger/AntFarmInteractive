    <td><%= project.presentation_title %></td>
    <td style="padding-right: 0"><%=link_to edit_project_path(project) do %>
      <i class="icon-edit icon"></i>
      <% end %></td>
    <td><%=link_to edit_project_path(project) do %><b>Edit</b><% end %>
